<html>
<head>
	<meta charset="utf-8">
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
    <link href='http://fonts.googleapis.com/css?family=Lato:100,400,300' rel='stylesheet' type='text/css'>
	<script src="/javascripts/ct.js"></script>
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <title>CourseTackle</title>
</head>
<header>
	<div id="navigation">
		<span id="title" width="10%">
			CourseTackle
		</span>
		<span class="links">
			<a href='/newsfeed'>home</a>
		</span>
		<span class="links">
			<a href='/profile'>profile</a>
		</span>
		<span class="links">
			<a href='/course'>courses</a>
		</span>
		<span class="search">
			<textarea id="search_bar" onclick="this.focus();this.select()">search...</textarea>
		</span>
		<span class="links">
			<a href='/'>sign out</a>
		</span>
	</div>
</header>
<body>
	<div id="left_panel" class="main" onload="setup_profile()">
		<span id="display_pic">
			<img src="/images/default.jpg" id="dp">
		</span>
		<span id="username_display">default</span>
		<span id="name_display">default</span>
		<span id="email_display">default</span>
		<span id="feild_display">default</span>
		<span id="friends">friends:</span>
	</div>
	<div id="courses" class="main">
		<h4>courses</h4>
		<span id="taken" class="post">
			<text id="add_course_taken" class="add">+</text>
			courses you've taken
		</span><span id="to_take" class="post">
			<text id="add_course_to_take" class="add">+</text>
			courses you will take
		</span>
	</div>
</body>
</html>